<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:androidprv="http://schemas.android.com/apk/prv/res/android">
    <item android:id="@id/indicator_solid">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/primary_text_disable_only_holo_light" />
            <corners android:radius="28.0dip" />
        </shape>
    </item>
    <item android:id="@id/indicator_stroke">
        <shape android:shape="rectangle">
            <corners android:radius="28.0dip" />
            <stroke android:width="2.0dip" android:color="@android:color/primary_text_disable_only_holo_light" />
        </shape>
    </item>
</layer-list>
